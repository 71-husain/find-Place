

  
 <%  if(success && success.length){%>
    <!-- <div class="alert alert-success" role="alert">
    
    </div> -->
    <div class=" alert alert-success align-items-center alert-dismissible fade show col-6 offset-3 mt-3" role="alert" >
            <%= success %>
          
          <button
           type="button" 
           class="btn-close btn-close-green me-2 m-auto" 
           data-bs-dismiss="alert" 
           aria-label="Close">
          </button>
    </div>
  <% } %>

<%  if(error && error.length){%>
  <div class=" alert alert-success align-items-center alert-dismissible fade show col-6 offset-3 mt-3" role="alert" >
    <%= error %>
  
  <button
   type="button" 
   class="btn-close btn-close-green me-2 m-auto" 
   data-bs-dismiss="alert" 
   aria-label="Close">
  </button>
</div>
<% } %>
  